* BEGIN - SECTION 1a - IMPORTS THE GRADUATE FIXED LENGTH FILES *;

DATA Grad01 (KEEP=
					ADCPersonID
					ADCGradLastName
					ADCGradFirstName
					ADCGradMiddleName
					ADCGradTerm
					ADCGradAcadProgram
					ADCGradProgramCIP
					ADCGradAddress
					ADCGradCity
					ADCGradState
					ADCGradZip
					ADCGradCountry
					ADCPersonID2
);
INFILE "&pt01&tx01" LINESIZE=401 TRUNCOVER;
INPUT @1 ADCPersonID $10. @;
	SELECT;
	WHEN (ADCPersonID NE 'ACAD.PERSO') DO;
		INPUT 
				@1		ADCPersonID				$10.
				@15		ADCGradLastName			$25.
				@40		ADCGradFirstName		$15.
				@55		ADCGradMiddleName		$15.
				@70		ADCGradTerm				$10.
				@105	ADCGradAcadProgram		$12.
				@125	ADCGradProgramCIP		$7.
				@141	ADCGradAddress			$30.
				@171	ADCGradCity				$30.
				@201	ADCGradState			$3.
				@206	ADCGradZip				$12.
				@221	ADCGradCountry			$4.
				@241	ADCPersonID2			$10.
;
		OUTPUT Grad01;
	END;
	OTHERWISE DO;
	PUT 'Error in record ' _n_ 'as follows:' _INFILE_;
		DELETE;
	END;
END;
RUN;

* END - SECTION 1a - IMPORTS THE GRADUATE FIXED LENGTH FILES *;

* BEGIN - SECTION 1b - REPEATS NECESSARY VARIABLE VALUES FOR SECTION 1a *;

DATA repeatids1b;
SET Grad01;
	RETAIN 	Item01;
	DROP	Item01;
  IF ADCPersonID EQ ' '
     THEN ADCPersonID=Item01;
     ELSE Item01=ADCPersonID;
*PROC FREQ DATA=repeatids1b ORDER=FREQ;
*	TABLES ADCPersonID;
RUN;

* END - SECTION 1b - REPEATS NECESSARY VARIABLE VALUES FOR SECTION 1a *;


* BEGIN - SECTION 1c - ARRAYS MULTI-VALUE ADDRESS LINES FROM SECTION 1b *;

DATA Address01a;
SET repeatids1b;
IF ADCPersonID2 NE ' ';
RUN;

DATA Address02a (KEEP=ADCPersonID ADCGradAddress);
SET repeatids1b;
IF ADCPersonID2 EQ ' ';
RUN;

DATA Address01b;
SET Address01a;
ADCGradAddress01=ADCGradAddress;
IF ADCGradCountry EQ ' '
	OR ADCGradCountry EQ 'USA'
	OR ADCGradCountry EQ 'US'
	THEN ADCGradCountry='US';
DROP ADCGradAddress;
PROC SORT DATA=Address01b; BY ADCPersonID ADCGradAcadProgram;
PROC SORT NODUPKEY DATA=Address01b; BY ADCPersonID ADCGradAcadProgram;
PROC SORT DATA=Address01b; BY ADCPersonID;
RUN;

DATA Address02b;
SET Address02a;
ADCGradAddress02=ADCGradAddress;
IF ADCGradAddress02 EQ ' ' THEN DELETE;
DROP ADCGradAddress;
PROC SORT DATA=Address02b; BY ADCPersonID;
PROC SORT NODUPKEY DATA=Address02b; BY ADCPersonID;
RUN;

DATA MergeAddress01a;
	MERGE Address01b(IN=a) Address02b(IN=b); BY ADCPersonID;
	IF a*b=1;
RUN;

DATA MergeAddress01b;
	MERGE Address01b(IN=a) Address02b(IN=b); BY ADCPersonID;
	IF a AND NOT b;
RUN;

DATA MergeAddress01c;
SET MergeAddress01a
	MergeAddress01b;
PROC SORT DATA=MergeAddress01c; BY ADCPersoniD;
RUN;

* END - SECTION 1c - ARRAYS MULTI-VALUE ADDRESS LINES FROM SECTION 1b *;


